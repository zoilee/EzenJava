package chpt05.sec03;

public class AdvancedFor_Exam {

	public static void main(String[] args) {
		int[] scores = {95,71,84,93,87};
		int hub = 0;
		int total = 0;
		for(int adv : scores) {
			hub += adv;
			total++;
		}
		
		double avg = (double) hub/total ;
	
		System.out.println(hub);		
		System.out.println(avg);

	}

}
