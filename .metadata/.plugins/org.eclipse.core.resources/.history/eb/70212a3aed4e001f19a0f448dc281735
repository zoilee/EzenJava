package chpt04.sec02;

public class Count_Exam {

	public static void main(String[] args) {
		int i = 0;
		float f = 0;
		for(i = 0; i<=10;i++) {
			System.out.println(i);
		}
		for(float x = 1.0f; x <= 1.0f; x+=0.1f) {
			System.out.println(x);
		}						
			
		for(i = 0; i<=10;i++) {
			System.out.println((f+i));
		}

	}

}
