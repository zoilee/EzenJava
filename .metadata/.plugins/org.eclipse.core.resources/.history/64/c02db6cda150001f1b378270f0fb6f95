package chpt06.sec01;

public class Car_Exam {

	public static void main(String[] args) {
		
		
		//Car 클래스를 이용해 객체를 생성
		Car myCar = new Car();
		
		//Car 객체의 필드값
		myCar.model = "현대";
		myCar.start = true;
		myCar.speed = 30;
		System.out.println("모델명 :" + myCar.model);
		System.out.println("시동여부 :" + myCar.start);
		System.out.println("현재속도 :" + myCar.speed);
		
	}

}
