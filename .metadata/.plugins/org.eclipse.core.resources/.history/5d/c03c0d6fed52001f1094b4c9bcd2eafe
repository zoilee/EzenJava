package chpt08.sec02;

public class Car {
	//필드선언
	String company = "현대자동차";
	String model;
	String color;
	int maxSpeed;
	
	//생성자
	Car(String model){
		this.model = "model";
	};
	Car(String model, String color){
		this.model = "model";
		this.color = "은색";
	}
	Car(String model, String color, int maxSpeed){
		this.model = model;
		this.color = color;
		this.maxSpeed = 250;
	}
}
