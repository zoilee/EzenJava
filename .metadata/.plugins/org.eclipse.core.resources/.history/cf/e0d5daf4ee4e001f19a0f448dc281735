package chpt04.sec02;

public class Multiple_Exam {

	public static void main(String[] args) {
		for ( int i = 1; i<=9; i++) {
			switch(i) {
			case 2:
				System.out.println("**********"+i+"´Ü *******");
				for(int j = 1; j<=9; j++) {
				
					System.out.println(i + "X" + j +"=" + (i*j));
			break;
			}
			System.out.println("**********"+i+"´Ü *******");
			for(int j = 1; j<=9; j++) {
			
				System.out.println(i + "X" + j +"=" + (i*j));
			}
		}

	}

}
